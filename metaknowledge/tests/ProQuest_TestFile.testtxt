        abstract={Incorporating installation, performance, and online/interactive media, the Temple of Science is a holistic socially engaged art project that examines the hegemony of science in modern Western culture.}, keywords={Philosophy, religion and theology; Communication and the arts; Latour, BRuno; Epistemology; Installation; Interactive media; Performance; Original artwork; Religion; Philosophy; Fine arts; Philosophy of Science; 0357:Fine arts; 0322:Religion; 0402:Philosophy of Science; 0322:Philosophy},
        isbn={9781303974342}, language={English},
